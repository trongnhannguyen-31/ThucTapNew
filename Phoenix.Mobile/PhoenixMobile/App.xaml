        <!--full page control template-->
        <ControlTemplate x:Key="FullScreenHasBack">
            <Grid VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand"
                  >
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                </Grid.RowDefinitions>
                <controls1:NavigationBar Grid.Row="0" Style="{StaticResource NavigationBarStandard}"
                                         HasLeftButton="True"
                                         BackCommand="{TemplateBinding BindingContext.BackCommand}"
                                         TitleDisplay="{TemplateBinding BindingContext.CurrentPage.Title}"
                                         TitleFontSize="{DynamicResource Title3FontSize}"/>
                <ScrollView Grid.Row="1" IsVisible="{TemplateBinding BindingContext.IsBusy,Converter={x:Static converters:InverseBoolConverter.Instance}}">
                    <ContentPresenter VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                        <ContentPresenter.Effects>
                            <effects:SafeAreaPaddingEffect/>
                        </ContentPresenter.Effects>
                    </ContentPresenter>
                </ScrollView>

            </Grid>

        </ControlTemplate>
        <ControlTemplate x:Key="FullScreenNoBack">
            <Grid VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand"
                  >
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <controls1:NavigationBar Grid.Row="0" Style="{StaticResource NavigationBarStandard}"
                                     HasLeftButton="False"                                     
                                     TitleDisplay="{TemplateBinding BindingContext.CurrentPage.Title}"
                                         TitleFontSize="{DynamicResource Title3FontSize}"/>
                <ContentPresenter Grid.Row="1" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                    <ContentPresenter.Effects>
                        <effects:SafeAreaPaddingEffect/>
                    </ContentPresenter.Effects>
                </ContentPresenter>
            </Grid>

        </ControlTemplate>
        <ControlTemplate x:Key="FullScreenTemplate">
            <Grid VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand"
                  >
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                </Grid.RowDefinitions>
                <controls1:NavigationBar Grid.Row="0" Style="{StaticResource NavigationBarStandard}"
                                         HasLeftButton="True"
                                         BackCommand="{TemplateBinding BindingContext.BackCommand}"
                                         TitleDisplay="{TemplateBinding BindingContext.CurrentPage.Title}"
                                         TitleFontSize="{DynamicResource Title3FontSize}"/>
                <ContentPresenter Grid.Row="1" IsVisible="{TemplateBinding BindingContext.IsBusy,Converter={x:Static converters:InverseBoolConverter.Instance}}">
                    <ContentPresenter.Effects>
                        <effects:SafeAreaPaddingEffect/>
                    </ContentPresenter.Effects>
                </ContentPresenter>
                <!--<ActivityIndicator Grid.Row="1" IsVisible="{TemplateBinding BindingContext.IsBusy}"
                        IsRunning="true"
                        HorizontalOptions="Center"
                        VerticalOptions="Center" />-->


            </Grid>
        </ControlTemplate>
        <ControlTemplate x:Key="ModalFullScreenHasBack">
            <Grid VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand"
                  >
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                </Grid.RowDefinitions>
                <controls1:NavigationBar Grid.Row="0" Style="{StaticResource NavigationBarStandard}"
                                         HasLeftButton="True"
                                         BackCommand="{TemplateBinding BindingContext.BackModalCommand}"
                                         TitleDisplay="{TemplateBinding BindingContext.CurrentPage.Title}"
                                         TitleFontSize="{DynamicResource Title3FontSize}"/>
                <ContentPresenter Grid.Row="1" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                    <ContentPresenter.Effects>
                        <effects:SafeAreaPaddingEffect/>
                    </ContentPresenter.Effects>
                </ContentPresenter>

            </Grid>

        </ControlTemplate>
